n                        material_rar: \"rare\",\r\n                        number: \"x80000\",\r\n                    },\r\n                ],\r\n            },\r\n            {\r\n                id: 5,\r\n                phase: 4,\r\n                lvl_s: '60+',\r\n                lvl_e: 70,\r\n                hp_s: 7220,\r\n                hp_e: 7936,\r\n                atk_s: 236,\r\n                atk_e: 260,\r\n                def_s: 396,\r\n                def_e: 435,\r\n                sp_stat: \"14.6%\",\r\n                materials: [\r\n                    {\r\n                        material: \"vajrada amethyst chunk\",\r\n                        material_rar: \"epic\",\r\n                        number: \"x6\",\r\n                    },\r\n                    {\r\n                        material: \"dragonheir's_false_fin\",\r\n                        material_rar: \"epic\",\r\n                        number: \"x12\",\r\n                    },\r\n                    {\r\n                        material: \"sea_ganoderma\",\r\n                        material_rar: \"trash\",\r\n                        number: \"x45\",\r\n                    },\r\n                    {\r\n                        material: \"famed handguard\",\r\n                        material_rar: \"rare\",\r\n                        number: \"x12\",\r\n                    },\r\n                    {\r\n                        material: \"mora\",\r\n                        material_rar: \"rare\",\r\n                        number: \"x100000\",\r\n                    },\r\n                ],\r\n            },\r\n            {\r\n                id: 6,\r\n                phase: 5,\r\n                lvl_s: '70+',\r\n                lvl_e: 80,\r\n                hp_s: 8428,\r\n                hp_e: 9151,\r\n                atk_s: 276,\r\n                atk_e: 300,\r\n                def_s: 462,\r\n                def_e: 502,\r\n                sp_stat: \"19.4%\",\r\n                ma